		"supertest": "^6.3.3",
		"ts-jest": "^29.0.3",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@types/express": "^4.17.15",
		"csv-parse": "^4.14.1",
		"express": "^4.18.2",
